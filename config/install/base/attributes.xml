<?xml version="1.0"?>
<concrete5-cif version="1.0">

    <attributetypes>
        <attributetype handle="fileset" name="Fileset" package="theme_supermint">
            <categories>
                <category handle="collection"/>
            </categories>
        </attributetype>
        <attributetype handle="supermint_preset_list" name="Supermint Theme Preset List" package="theme_supermint">
            <categories>
                <category handle="collection"/>
            </categories>
        </attributetype>
        <attributetype handle="color" name="Color" package="theme_supermint">
            <categories>
                <category handle="collection"/>
            </categories>
        </attributetype>
        <attributetype handle="icon_awesome" name="Icon From FontAwesome" package="theme_supermint">
            <categories>
                <category handle="collection"/>
            </categories>
        </attributetype>
    </attributetypes>

    <attributekeys>
        <attributekey handle="page_backgrounds" name="Slide Background" package="theme_supermint" searchable="0" indexed="0" type="fileset" category="collection"/>
        <attributekey handle="page_background" name="Page Background" package="theme_supermint" searchable="1" indexed="1" type="image_file" category="collection"/>
        <attributekey handle="thumbnail" name="Thumbnail" package="theme_supermint" searchable="1" indexed="1" type="image_file" category="collection"/>
        <attributekey handle="main_page_color" name="Main page color" package="theme_supermint" searchable="1" indexed="1" type="color" category="collection"/>
        <attributekey handle="icon" name="Icon from fontAwesome" package="theme_supermint" searchable="0" indexed="1" type="icon_awesome" category="collection"/>
        <attributekey handle="supermint_theme_preset_options" name="Theme preset Options" package="theme_supermint" searchable="1" indexed="1" type="supermint_preset_list" category="collection"/>
        <attributekey handle="supermint_navigation_type" name="SLide Navigation Type" package="theme_supermint" searchable="1" indexed="1" type="select" category="collection">
            <type allow-multiple-values="0" display-order="display_asc" allow-other-values="0">
                <options>
                    <option value="Type 1 (L1-2 w desc)" is-end-user-added="0"/>
                    <option value="Type 2 (L1-2-3)" is-end-user-added="0"/>
                    <option value="Type 3 (L1-2)" is-end-user-added="0"/>
                    <option value="Hidden" is-end-user-added="0"/>
                </options>
            </type>
        </attributekey>
        <attributekey handle="display_multi_columns_drop" name="Display multi-columns dropdown" package="" searchable="0" indexed="0" type="boolean" category="collection"><type checked=""/></attributekey>
        <attributekey handle="number_of_main_areas" name="Number of main areas" package="theme_supermint" searchable="0" indexed="1" type="select" category="collection">
            <type allow-multiple-values="0" display-order="display_asc" allow-other-values="0">
                <options>
                    <option value="2" is-end-user-added="0"/>
                    <option value="3" is-end-user-added="0"/>
                    <option value="4" is-end-user-added="0"/>
                    <option value="5" is-end-user-added="0"/>
                    <option value="6" is-end-user-added="0"/>
                    <option value="7" is-end-user-added="0"/>
                </options>
            </type>
        </attributekey>
        <attributekey handle="number_of_wide_columns" name="Number of columns on Full Wide" package="theme_supermint" searchable="0" indexed="1" type="select" category="collection">
            <type allow-multiple-values="0" display-order="display_asc" allow-other-values="0">
                <options>
                    <option value="1" is-end-user-added="0"/>
                    <option value="2" is-end-user-added="0"/>
                    <option value="3" is-end-user-added="0"/>
                    <option value="4" is-end-user-added="0"/>
                    <option value="6" is-end-user-added="0"/>
                </options>
            </type>
        </attributekey>
        <attributekey handle="layout_mode" name="Layout Mode" package="theme_supermint" searchable="0" indexed="1" type="select" category="collection">
            <type allow-multiple-values="0" display-order="display_asc" allow-other-values="0">
                <options>
                    <option value="default" is-end-user-added="0"/>
                    <option value="boxed-wrapper" is-end-user-added="0"/>
                    <option value="wide" is-end-user-added="0"/>
                </options>
            </type>
        </attributekey>
        <attributekey handle="review_occupation" name="Occupation" package="theme_supermint" searchable="1" indexed="1" type="text" category="collection"/>
        <attributekey handle="review_website" name="Office Website" package="theme_supermint" searchable="1" indexed="1" type="text" category="collection"/>
        <attributekey handle="review_website_url" name="Office Website URL" package="theme_supermint" searchable="1" indexed="1" type="text" category="collection"/>
        <attributekey handle="review_name" name="Name" package="theme_supermint" searchable="1" indexed="1" type="text" category="collection"/>
        <attributekey handle="ribbon_text" name="Text for the ribbon" package="theme_supermint" searchable="0" indexed="0" type="text" category="collection"/>
        <attributekey handle="exclude_subpages_from_nav" name="Exlude Subpages from navigation" package="" searchable="0" indexed="0" type="boolean" category="collection"/>
        <attributekey handle="replace_link_with_first_in_nav" name="Replace Link with First in Nav" package="" searchable="0" indexed="0" type="boolean" category="collection"/>
        <attributekey handle="external_link" name="Use a external URL as link" package="theme_supermint" searchable="0" indexed="0" type="text" category="collection"/>

    </attributekeys>

    <attributesets>
        <attributeset handle="supermint_page" name="Supermint Page" package="theme_supermint" locked="0" category="collection">
            <attributekey handle="thumbnail"/>
            <attributekey handle="main_page_color"/>
            <attributekey handle="icon"/>
            <attributekey handle="layout_mode"/>
            <attributekey handle="page_backgrounds"/>
            <attributekey handle="page_background"/>
            <attributekey handle="number_of_main_areas"/>
            <attributekey handle="number_of_wide_columns"/>
            <attributekey handle="ribbon_text"/>
        </attributeset>
        <attributeset handle="supermint_navigation" name="Supermint Navigation" package="theme_supermint" locked="0" category="collection">
            <attributekey handle="supermint_navigation_type"/>
            <attributekey handle="display_multi_columns_drop"/>
        </attributeset>
        <attributeset handle="supermint_options" name="Supermint Options" package="theme_supermint" locked="0" category="collection">
            <attributekey handle="supermint_theme_preset_options"/>
        </attributeset>
        <attributeset handle="supermint_testimonial" name="Supermint Testimonials" package="theme_supermint" locked="0" category="collection">
            <attributekey handle="review_name"/>
            <attributekey handle="review_occupation"/>
            <attributekey handle="review_website"/>
            <attributekey handle="review_website_url"/>
        </attributeset>
        <attributeset handle="navigation" category="collection">
            <attributekey handle="exclude_subpages_from_nav"/>
            <attributekey handle="replace_link_with_first_in_nav"/>
            <attributekey handle="external_link"/>
        </attributeset>
    </attributesets>
</concrete5-cif>
